#!/usr/bin/env gnuplot

set terminal png size 6400, 800
set output 'wave.png'

set xr [14500:15400]

plot 'wave.csv' using 0:($1*0.008 - 50) with lines, \
     'wave2.csv' using 0:($3*0.01 - 12) with lines, \
     'wave2.csv' using 0:($4) with lines, \
     'wave2.csv' using 0:($5) with lines, \
     'wave2.csv' using 0:(-12):6 with labels
